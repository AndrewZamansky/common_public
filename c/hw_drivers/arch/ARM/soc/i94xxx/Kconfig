menu "`--->  I94XXX board configurations"

choice
	prompt "Board"
	default I94XXX_SVB

config I94XXX_SVB
	bool "I94XXX SVB board"

endchoice

if I94XXX_SVB
source "$KCONFIG_START_DIR_PATH/hw_drivers/arch/ARM/soc/i94xxx/Kconfig.svbI94120.dt"
endif



endmenu

=======
config I94XXX_SVB
	def_bool y
	select INTERNAL_SRAM 
	select INTERNAL_FLASH 
	select CORTEX_M4 
	select SOC_HAS_DSP
