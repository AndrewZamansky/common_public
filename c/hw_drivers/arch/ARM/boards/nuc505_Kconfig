menu "nuc505 basic configurations"

config CORTEX_M4
	def_bool y

config RAM_START_ADDR
    hex "ram start address (hex)"
    default 0x20000000

config RAM_SIZE
    hex "ram size (hex)"
    default 0x20000    

config FLASH_START_ADDR
    hex "flash start address (hex)"
    default 0x0

config FLASH_SIZE
    hex "flash size (hex)"
    default 0x40000   
endmenu
