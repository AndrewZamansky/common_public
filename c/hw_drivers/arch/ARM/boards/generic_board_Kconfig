menu "`--->  generic basic configurations"

choice
	prompt "Cpu Type"
	default CPU_UNDEFINED

config CORTEX_M3
	bool "cortex-M3"

config CORTEX_M4
	bool "cortex-M4"

config CORTEX_A9
    bool "cortex-A9"

config ARM_926EJ_S
	bool "arm926ej-s"

config CPU_UNKNOWN
	bool "unknown cpu"

endchoice

endmenu

config GENERIC_BOARD
	def_bool y
