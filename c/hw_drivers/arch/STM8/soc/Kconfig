

choice
	prompt "SOC"
	default GENERIC_STM8_SOC

config STM8S103F
	bool "STM8S103F"

config GENERIC_STM8_SOC
	bool "generic SOC"

endchoice

if STM8S103F
source "$KCONFIG_START_DIR_PATH/hw_drivers/arch/STM8/soc/stm8s103f/Kconfig"
endif

if GENERIC_STM8_SOC
source "$KCONFIG_START_DIR_PATH/hw_drivers/arch/STM8/soc/generic_board_Kconfig"
endif
