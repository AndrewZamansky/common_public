#

menu "net"

menuconfig INCLUDE_CURL
	bool   "curl"
	default n

if INCLUDE_CURL
source "$COMMON_DIR_PATH/c/sw_packages/net/curl/Kconfig"
endif

menuconfig INCLUDE_OPENSSL
	bool   "OpenSSL"
	default n

if INCLUDE_OPENSSL
source "$COMMON_DIR_PATH/c/sw_packages/net/openssl/Kconfig"
endif

menuconfig INCLUDE_WOLFSSL
	bool   "wolfSSL"
	default n

if INCLUDE_WOLFSSL
source "$COMMON_DIR_PATH/c/sw_packages/net/wolfssl/Kconfig"
endif

menuconfig INCLUDE_NGHTTP2
	bool   "nghttp2"
	default n

if INCLUDE_NGHTTP2
source "$COMMON_DIR_PATH/c/sw_packages/net/nghttp2/Kconfig"
endif


config  CURL_CUSTOM_SOCKET_LAYER
	bool   "custom socket layer"
	default n


endmenu
